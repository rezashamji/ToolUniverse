      "properties": {
        "dataset_id": {
          "type": "string",
          "description": "GEO dataset ID (e.g., 'GDS1234', 'GSE12345')"
        }
      },
      "required": ["dataset_id"]
    },
    "fields": {
      "endpoint": "/esummary.fcgi",
      "return_format": "JSON"
    },
    "return_schema": {
      "type": "object",
      "description": "GEO sample information",
      "properties": {
        "result": {
          "type": "object",
          "description": "Sample information from GEO",
          "properties": {
            "uid": {
              "type": "string",
              "description": "Dataset UID"
            },
            "samples": {
              "type": "array",
              "description": "Array of sample information",
              "items": {
                "type": "object",
                "properties": {
                  "sample_id": {
                    "type": "string",
                    "description": "Sample identifier"
                  },
                  "characteristics": {
                    "type": "string",
                    "description": "Sample characteristics"
                  },
                  "treatment": {
                    "type": "string",
                    "description": "Treatment conditions"
                  }
                }
              }
            }
          }
        }
      }
    },
    "test_examples": [
      {
        "dataset_id": "GDS1234"
      },
      {
        "dataset_id": "GSE12345"
      }
    ],
    "label": [
      "GEO",
      "Sample",
      "Information"
    ],
    "metadata": {
      "tags": ["sample", "characteristics", "expression"],
      "difficulty_level": "easy",
      "estimated_execution_time": "< 3 seconds"
    }
  }
]

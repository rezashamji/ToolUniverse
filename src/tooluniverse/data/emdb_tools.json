[
  {
    "type": "EMDBRESTTool",
    "name": "EMDB_get_structure",
    "description": "Get electron microscopy structure data from EMDB",
    "parameter": {
      "type": "object",
      "properties": {
        "emdb_id": {"type": "string", "description": "EMDB structure ID (e.g., EMD-1234)"}
      },
      "required": ["emdb_id"]
    },
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/emdb/api/entry/{emdb_id}",
      "return_format": "JSON"
    },
    "return_schema": {
      "type": "object",
      "properties": {
        "emdb_id": {"type": "string"},
        "title": {"type": "string"},
        "description": {"type": "string"},
        "resolution": {"type": "number"},
        "method": {"type": "string"},
        "organism": {"type": "string"},
        "molecule": {"type": "string"},
        "deposition_date": {"type": "string"},
        "release_date": {"type": "string"},
        "authors": {"type": "array"},
        "journal": {"type": "string"},
        "volume": {"type": "string"},
        "page": {"type": "string"},
        "year": {"type": "integer"},
        "doi": {"type": "string"},
        "pdb_id": {"type": "string"},
        "map_url": {"type": "string"},
        "contour_level": {"type": "number"},
        "voxel_size": {
          "type": "object",
          "properties": {
            "x": {"type": "number"},
            "y": {"type": "number"},
            "z": {"type": "number"}
          }
        },
        "map_dimensions": {
          "type": "object",
          "properties": {
            "x": {"type": "integer"},
            "y": {"type": "integer"},
            "z": {"type": "integer"}
          }
        }
      }
    },
    "test_examples": [
      {"emdb_id": "EMD-1234"},
      {"emdb_id": "EMD-0001"}
    ]
  }
]

        "status": {
          "type": "string",
          "description": "Status of the request"
        },
        "data": {
          "type": "array",
          "description": "Array of pathway results",
          "items": {
            "type": "object",
            "properties": {
              "pathway_id": {
                "type": "string",
                "description": "KEGG pathway identifier"
              },
              "description": {
                "type": "string",
                "description": "Pathway description"
              }
            }
          }
        },
        "count": {
          "type": "integer",
          "description": "Number of pathways found"
        },
        "url": {
          "type": "string",
          "description": "API URL used"
        }
      }
    },
    "test_examples": [
      {
        "keyword": "diabetes"
